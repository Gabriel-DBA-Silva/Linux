# rkB/s

sar -d 1 1 | grep -v DEV | grep Average | awk '{print "zabbix_sender -z 127.0.0.1 -p 10051 -s \"Gabriel Linux\" -k custom.dynamic["$2",rkBs]", "-o " $4}'

# wkB/s 

sar -d 1 1 | grep -v DEV | grep Average | awk '{print "zabbix_sender -z 127.0.0.1 -p 10051 -s \"Gabriel Linux\" -k custom.dynamic["$2",wkBs]", "-o " $5}'

# %util

